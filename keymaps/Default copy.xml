<keymap version="1" name="Default copy" parent="$default">
  <action id="Back">
    <keyboard-shortcut first-keystroke="ctrl alt left" />
    <mouse-shortcut keystroke="button4" />
    <keyboard-shortcut first-keystroke="ctrl alt numpad4" />
  </action>
  <action id="CommentByLineComment">
    <keyboard-shortcut first-keystroke="ctrl slash" />
    <keyboard-shortcut first-keystroke="ctrl divide" />
    <keyboard-shortcut first-keystroke="ctrl 7" />
  </action>
  <action id="EditorDeleteToWordEnd" />
  <action id="EditorDeleteToWordEndInDifferentHumpsMode">
    <keyboard-shortcut first-keystroke="ctrl delete" />
  </action>
  <action id="EditorDeleteToWordStart" />
  <action id="EditorDeleteToWordStartInDifferentHumpsMode">
    <keyboard-shortcut first-keystroke="ctrl back_space" />
  </action>
  <action id="EditorNextWord" />
  <action id="EditorNextWordInDifferentHumpsMode">
    <keyboard-shortcut first-keystroke="ctrl right" />
  </action>
  <action id="EditorNextWordInDifferentHumpsModeWithSelection">
    <keyboard-shortcut first-keystroke="shift ctrl right" />
  </action>
  <action id="EditorNextWordWithSelection" />
  <action id="EditorPreviousWord" />
  <action id="EditorPreviousWordInDifferentHumpsMode">
    <keyboard-shortcut first-keystroke="ctrl left" />
  </action>
  <action id="EditorPreviousWordInDifferentHumpsModeWithSelection">
    <keyboard-shortcut first-keystroke="shift ctrl left" />
  </action>
  <action id="EditorPreviousWordWithSelection" />
  <action id="EditorScrollToCenter" />
  <action id="Forward">
    <keyboard-shortcut first-keystroke="ctrl alt right" />
    <mouse-shortcut keystroke="button5" />
    <keyboard-shortcut first-keystroke="ctrl alt numpad6" />
  </action>
  <action id="GotoBookmark7" />
  <action id="HippieBackwardCompletion">
    <keyboard-shortcut first-keystroke="shift alt slash" />
    <keyboard-shortcut first-keystroke="shift alt 8" />
  </action>
  <action id="HippieCompletion">
    <keyboard-shortcut first-keystroke="alt slash" />
    <keyboard-shortcut first-keystroke="shift alt 7" />
  </action>
  <action id="IncrementWindowWidth" />
  <action id="MakeModule">
    <keyboard-shortcut first-keystroke="ctrl m" />
  </action>
  <action id="Refresh">
    <keyboard-shortcut first-keystroke="alt f5" />
  </action>
  <action id="ResizeToolWindowLeft" />
  <action id="ResizeToolWindowRight" />
  <action id="RevealIn">
    <mouse-shortcut keystroke="control button1 doubleClick" />
    <mouse-shortcut keystroke="control button3" />
    <keyboard-shortcut first-keystroke="shift ctrl x" />
  </action>
  <action id="ScrollTreeToCenter" />
  <action id="SelectInProjectView">
    <keyboard-shortcut first-keystroke="ctrl dead_circumflex" />
  </action>
  <action id="ShowIntentionActions">
    <keyboard-shortcut first-keystroke="alt enter" />
    <keyboard-shortcut first-keystroke="ctrl 1" />
  </action>
  <action id="Tool_External Tools_update local-env">
    <keyboard-shortcut first-keystroke="ctrl alt x" second-keystroke="u" />
  </action>
  <action id="Vcs.ShowMessageHistory">
    <keyboard-shortcut first-keystroke="ctrl e" />
  </action>
</keymap>